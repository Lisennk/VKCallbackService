{
  "extends": "airbnb-base",
  "plugins": [
      "import"
  ],
  "rules": {
    "indent": 0,
    "no-unused-vars": "warn",
    "space-before-function-paren": ["error", "always"],
    "class-methods-use-this": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "prefer-template": 0,
    "import/no-dynamic-require": "warn",
    "no-param-reassign": 0
  }
}